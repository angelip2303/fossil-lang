// this file does not exist, it must raise an error
let input = csv!("tests/RMLTC0002e-CSV/student2.csv")

type Output do
    @rdf(uri = "http://example.com/id")
    ID: string

    @rdf(uri = "http://xmlns.com/foaf/0.1/name")
    Name: string
end

input
|> map(fn row -> Output {
    @id = "http://example.com/${row.ID}/${row.Name}",
    ID = row.IDs,
    Name = row.Name
} end)
|> Rdf.serialize("tests/RMLTC0002e-CSV/actual.nq")
