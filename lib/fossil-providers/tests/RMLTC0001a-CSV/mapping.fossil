let input = csv!("tests/RMLTC0001a-CSV/student.csv")

@rdf(base = "http://example.com/")
type Output(subject: string) do
    @rdf(uri = "http://xmlns.com/foaf/0.1/name")
    Name: string
end

input
|> fn row -> Output(row.Name) { Name = row.Name } end
|> Rdf.serialize("tests/RMLTC0001a-CSV/actual.nq")
