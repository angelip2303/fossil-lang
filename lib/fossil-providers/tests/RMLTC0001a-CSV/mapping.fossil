type Input = csv!("tests/RMLTC0001a-CSV/student.csv")

type Output = {
    #[rdf(uri = "http://xmlns.com/foaf/0.1/name")]
    Name: string,
}

Input::load()
|> map(fn(row) -> Output {
    @id = "http://example.com/${row.Name}",
    Name = row.Name
})
|> Rdf::serialize("tests/RMLTC0001a-CSV/actual.nq")
