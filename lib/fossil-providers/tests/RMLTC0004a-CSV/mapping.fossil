let student_sport = csv!("tests/RMLTC0004a-CSV/student_sport.csv")

#[rdf(type = "http://example.com/Student")]
type Student(subject: string) do
    #[rdf(uri = "http://xmlns.com/foaf/0.1/name")]
    Name: string
end

#[rdf(type = "http://example.com/Sport")]
type Sport(subject: string) do
    #[rdf(uri = "http://xmlns.com/foaf/0.1/name")]
    Name: string
end

student_sport
|> each row -> Student("http://example.com/${row.Student}") { Name = row.Student }
+> each row -> Sport("http://example.com/${row.Sport}") { Name = row.Sport }
|> Rdf.serialize("tests/RMLTC0004a-CSV/actual.nq")
