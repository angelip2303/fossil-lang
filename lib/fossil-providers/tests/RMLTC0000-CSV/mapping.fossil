type Input = csv!("tests/RMLTC0000-CSV/student.csv")

#[rdf(id = "http://example.com/${Name}")]
type Output = {
    #[rdf(uri = "http://xmlns.com/foaf/0.1/name")]
    Name: string,
}

let mapping = fn(row) -> Output(row.Name)

Input::load()
|> List::map(mapping)
|> Rdf::serialize("tests/RMLTC0000-CSV/actual.nq")
