#[rdf(type = "http://example.com/bim#Wall", base = "http://example.com/wall/")]
type ValidWall(id: string) do
    #[rdf("https://w3id.org/digitalconstruction/0.5/Entities#hasLabel")]
    Name: string

    #[rdf("http://example.com/bim#Description")]
    Description: string?
end

let walls = ifc!("lib/fossil-ifc/examples/basic-sample/building.ifc", "IfcWall")

walls
|> each wall -> ValidWall(wall.GlobalId) { Name = wall.Name, Description = wall.Description }
|> Rdf.serialize("output.nt")
