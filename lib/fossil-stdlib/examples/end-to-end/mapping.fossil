const ROOT = "lib/fossil-stdlib/examples/end-to-end"

type Data = csv!("${ROOT}/people.csv")

#[rdf(base = "http://example.com/person/")] // this should be automatically inferred
type Person(id: string) = shex!("${ROOT}/person.shex", shape: "PersonShape")

Data::load()
|> fn(row) -> Person(row.email) { name = row.name, age = row.age }
|> Rdf::serialize("actual.nq")
